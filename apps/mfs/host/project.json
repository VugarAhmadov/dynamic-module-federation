{
  "name": "host",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/mfs/host/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project host --web",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rsbuild dev --open",
        "cwd": "apps/mfs/host"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rsbuild build --open",
        "cwd": "apps/mfs/host"
      }
    }
  }
}
